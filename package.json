  "dependencies": {
    "chalk": "^2.3.0",
    "commander": "^2.11.0",
    "fs-extra": "^4.0.2",
    "get-installed-path": "^4.0.8",
    "node-cmd": "^3.0.0",
    "universalify": "^0.1.1"
  },
  "bugs": {
    "url": "https://github.com/shahzaibkhalid/mern-app-generator/issues"
